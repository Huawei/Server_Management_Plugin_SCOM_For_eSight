<Window
    x:Class="Huawei.SCOM.ESightPlugin.ViewLibTest.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Huawei.SCOM.ESightPlugin.ViewLibTest"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vl="clr-namespace:Huawei.SCOM.ESightPlugin.ViewLib;assembly=Huawei.SCOM.ESightPlugin.ViewLib"
    Title="MainWindow"
    Width="960"
    Height="450"
    Loaded="Window_Loaded"
    mc:Ignorable="d">

    <ScrollViewer
        HorizontalContentAlignment="Stretch"
        VerticalContentAlignment="Stretch"
        HorizontalScrollBarVisibility="Auto"
        VerticalScrollBarVisibility="Auto">
        <StackPanel>
            <vl:ESightConfigDashboard />
            <!--<vl:ESightHealthDashboard x:Name="HealthDashboard" />-->
        </StackPanel>
    </ScrollViewer>

</Window>
